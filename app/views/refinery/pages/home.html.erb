<div class="row">
   <section id='slider_content' class='eight columns'>
    <%=raw @page.content_for(:slider) %>
  </section>
  <section id='slider_side_body_content' class='four columns'>
    <%= raw @page.content_for(:slider_side_body) %>
  </section>
</div>
<div class="row">
  <section id='body_content' class='nine columns'> 
  <%=raw @page.content_for(:body) %>
  </section> 
  <section id='side_body_content' class='three columns'> 
  <%=raw @page.content_for(:side_body) %>
  </section> 
</div>

<% content_for :javascripts do %>
    <% javascript_include_tag('orbit.js') %>
<% end %>
